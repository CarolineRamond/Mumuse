{
	"env": {
		"test": {
			"presets": [["es2015"], "react"],
			"plugins": ["react-html-attrs", "transform-class-properties", "transform-decorators-legacy", "transform-object-rest-spread"]
		},
		"development": {
			"presets": ["@babel/preset-react", "@babel/preset-env"],
			"plugins": ["react-html-attrs", "transform-class-properties", "transform-decorators-legacy", "transform-object-rest-spread"]
		},
		"production": {
			"presets": ["@babel/preset-react", "@babel/preset-env"],
			"plugins": ["react-html-attrs", "transform-class-properties", "transform-decorators-legacy", "transform-object-rest-spread"]
		}
	}
}